# Alln1 Business Environment Configuration
# Copy this to .env and fill in your values

# ===========================================
# FIREBASE CONFIGURATION
# ===========================================

# Get these from Firebase Console > Project Settings > Your Apps
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=

# ===========================================
# AI CONFIGURATION (Optional)
# ===========================================

# OpenAI API Key for AI business assistant features
# Get one at: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# ===========================================
# REVENUECAT (Subscriptions)
# ===========================================

# Get these from RevenueCat Dashboard > Project Settings > API Keys
# Test keys start with "test_", production keys start with "appl_" / "goog_"
EXPO_PUBLIC_REVENUECAT_IOS_KEY=
EXPO_PUBLIC_REVENUECAT_ANDROID_KEY=
