{
  "expo": {
    "name": "Life OS",
    "slug": "lifeos-household",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "lifeos",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#0F0F0F",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lifeos.household"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0F0F0F"
      },
      "package": "com.lifeos.household"
    },
    "plugins": ["expo-router", "expo-secure-store"]
  }
}
